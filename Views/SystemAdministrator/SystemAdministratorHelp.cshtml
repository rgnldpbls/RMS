@{
    ViewData["Title"] = "FAQs";
    Layout = "SuperAdmin_Layout";
}
<link href="~/css/SystemAdministrator-FAQs.css" rel="stylesheet" />

<div>


    <div class="container display-6 mt-5 p-0">
        <div id="task">Frequently Asked Questions</div>
    </div>

    <div class="container mt-3">
        <div class="container shadow p-3" id="content">
            <div class="row align-items-center">
                <div class="lab col-10">
                    Question
                </div>
                <div class="col-2 text-end">
                    <button class="border-0" id="action" type="button" data-bs-toggle="collapse" data-bs-target="#faqcont" aria-expanded="false" aria-controls="faqcont">
                        <i class="fa-solid fa-circle-chevron-down fa-2xl" style="color: #850000;"></i>
                    </button>
                </div>
            </div>
        </div>
        <div class="collapse mt-3 bg-transparent" id="faqcont">
            <div class="card card-body container p-3" id="content">

                <p class="text-secondary">Answer</p>


            </div>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

</div>