@model ResearchManagementSystem.Models.AddFAQs

@{
    ViewData["Title"] = "Delete";
}
<link href="~/css/ClusterCoordinator-Accomplishmentdetails.css" rel="stylesheet" />
<style>
    #title {
        background: #850000;
        color: white;
    }
</style>
<div class="container shadow mb-3" id="content">
    <div class="row shadow-sm h3 p-4 mb-1" id="title">
        <div class="d-flex justify-content-between align-items-center">
            <div>
                <div class="d-block">Delete</div>
                <div class="fs-6">Are you sure you want to delete this?</div>
            </div>
            <div>
                <form asp-action="Delete">
                    <input type="hidden" asp-for="FAQ_id" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>

            </div>
        </div>
    </div>

    <dl class="row shadow p-4" id="formcont">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.superadmin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.superadmin.FullName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.question_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.question_id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.answer_id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.answer_id)
        </dd>
    </dl>
</div>


